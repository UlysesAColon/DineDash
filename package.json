{
  "name": "dinedash",
  "version": "1.0.0",
  "description": "DineDash App for RCB",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UlysesAColon/DineDash.git"
  },
  "author": "Ulyses Anthony Cornelius Drew",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UlysesAColon/DineDash/issues"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "homepage": "https://github.com/UlysesAColon/DineDash#readme",
  "dependencies": {
    "and": "0.0.3",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "bower": "^1.8.8",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.14",
    "npm": "^6.13.4",
    "postinstall": "^0.4.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-popper": "^1.3.3",
    "react-redux": "^6.0.1",
    "react-transition-group": "^2.5.3",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
